class A{
    public int x,y,z;
    public void sum(int a, int b){
        x=a;.
        y=b;
        z=x+y;
        System.out.println(z);
    }
}
class B extends A{
    public void multi(int a, int b){
      x=a;
      y=b;
      z=x*y;
      System.out.println(z);
    }
}

public class Example{
    public static void main(String[] args) {
        B obj=new B();
        obj.sum(2,3);
        obj.multi(3,4);

    }
}
